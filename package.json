{
  "name": "vpm-listing-generator",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.cts",
        "default": "./dist/*.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "ts-patch install && typia patch",
    "format": "npx @biomejs/biome check . --write",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [],
  "author": "narazaka",
  "license": "Zlib",
  "description": "",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@ryoppippi/unplugin-typia": "^1.1.0",
    "ts-patch": "^3.3.0",
    "tsup": "^8.3.5",
    "typescript": "~5.7.2"
  },
  "dependencies": {
    "octokit": "^4.0.3",
    "typia": "^7.5.1"
  }
}
